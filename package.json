{
  "name": "cyclejs_intro",
  "version": "1.0.0",
  "description": "An introduction to Cycle.js.",
  "main": "index.js",
  "scripts": {
    "build": "mkdirp public && browserify app/app.js -t babelify --outfile public/app.js",
    "watch": "mkdirp public && watchify app/app.js -t babelify --outfile public/app.js -v",
    "serve": "live-server ./public",
    "start": "npm-run-all --parallel watch serve"
  },
  "author": "jdtibbs",
  "license": "ISC",
  "dependencies": {
    "@cycle/core": "^6.0.2",
    "@cycle/dom": "^9.0.2",
    "rx": "^4.0.7"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "live-server": "^0.9.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^1.5.1",
    "shelljs": "^0.5.3",
    "tape": "^4.4.0",
    "uglify-js": "^2.6.1",
    "watchify": "^3.7.0"
  }
}
